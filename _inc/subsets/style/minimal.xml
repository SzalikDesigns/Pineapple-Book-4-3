<theme>
	<formatVersion>7</formatVersion>

	<variables>
		<!-- Default background colors -->
		<backgroundCustomColor1>fffff</backgroundCustomColor1>
		<backgroundCustomColor2>fffff</backgroundCustomColor2>
		
		<!-- Font Default color in menu -->
		<customcolorstandard>e6e6e6</customcolorstandard>

		<!-- Metadata backgrounds SVG -->
		<metadatamaxflipped>metadata-light/metadata-max-flipped</metadatamaxflipped>
		<metadatamax>metadata-light/metadata-max</metadatamax>
		<metadataminflipped>metadata-light/metadata-min-flipped</metadataminflipped>
		<metadatamin>metadata-light/metadata-min</metadatamin>
		<metadataoff>metadata-light/metadata-off</metadataoff>

		<!-- This option allows me to set different color overlays for the centered -->
		<airencolor>fullscreen-minimal</airencolor>
	</variables>

</theme>